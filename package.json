    "check-for-leaks": "^1.2.0",
    "cheerio": "^1.0.0-rc.2",
    "crowdin-glossary": "^1.2.0",
    "del": "^6.0.0",
    "dotenv-safe": "^8.1.0",
    "extract-zip": "^2.0.0",
    "get-crowdin-file-ids": "^1.1.1",
    "github-slugger": "^1.2.0",
    "globals": "^13.0.0",
    "got": "^11.0.1",
    "handlebars": "^4.7.4",
    "href-type": "^1.0.1",
