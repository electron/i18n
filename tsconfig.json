{
  "compilerOptions": {
    "target": "es2018",
    "module": "commonjs",
    "lib": [
      "es2018",
      "esnext"
    ],
    "strict": false,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "noUnusedLocals": false,
    "noImplicitAny": true,
    "noEmit": true
  }
}
